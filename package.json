{
  "name": "neko233-ts-sugar",
  "type": "module",
  "version": "1.0.37",
  "description": "typescript utils. 工具箱",
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "compile": "node nodejs-auto-create-export-index-ts.js && tsc",
    "gitPush": "git add . && git commit -m \"deploy version\" && git push ",
    "publishToNpm": "node nodejs-auto-create-export-index-ts.js  && npm version patch && tsc && npm publish --production ",
    "nextPublishVersion": " npm version patch  "
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "light.d.ts",
    "light.js",
    "minimal.d.ts",
    "minimal.js",
    "package-lock.json",
    "tsconfig.json",
    "scripts/postinstall.js",
    "dist/**",
    "ext/**",
    "google/**",
    "src/**"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/SolarisNeko/ts-sugar.git"
  },
  "keywords": [
    "typescript",
    "ts",
    "common",
    "utils",
    "neko233",
    "sugar"
  ],
  "author": "SolarisNeko",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
