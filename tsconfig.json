{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "sourceRoot": "./src",
    "outDir": "./dist",
    "declaration": true,
    "declarationDir": "./dist/typings",
    "experimentalDecorators": true,
    "noEmitHelpers": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,

  },
  "exclude": [
    "node_modules"
  ]
}